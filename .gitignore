# MERIDIEN - Git Ignore Rules

# Environment files
.env
.env.local
.env.*.local
*.env
!.env.example

# OS
.DS_Store
.DS_Store?
._*
.Spotlight-V100
.Trashes
ehthumbs.db
Thumbs.db
*~

# IDEs
.idea/
.vscode/
*.swp
*.swo
*.sublime-project
*.sublime-workspace

# Logs
*.log
logs/
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# Temporary files
tmp/
temp/
*.tmp

#######################
# Backend (Go)
#######################

# Binaries
backend/bin/
backend/dist/
*.exe
*.exe~
*.dll
*.so
*.dylib

# Test binary, built with `go test -c`
backend/*.test

# Output of the go coverage tool
backend/*.out
backend/coverage.html

# Go workspace file
go.work

# Vendor
backend/vendor/

# Air
backend/tmp/

# Swagger docs (generated)
backend/docs/swagger/

#######################
# Frontend (Flutter)
#######################

# Files and directories created by pub
frontend/.dart_tool/
frontend/.packages
frontend/build/
frontend/pubspec.lock

# Directory created by dartdoc
frontend/doc/api/

# Flutter/Dart/Pub related
frontend/**/doc/api/
frontend/.flutter-plugins
frontend/.flutter-plugins-dependencies
frontend/.pub-cache/
frontend/.pub/

# Web related
frontend/lib/generated_plugin_registrant.dart

# Symbolication related
frontend/app.*.symbols

# Obfuscation related
frontend/app.*.map.json

# Android related
frontend/**/android/**/gradle-wrapper.jar
frontend/**/android/.gradle
frontend/**/android/captures/
frontend/**/android/gradlew
frontend/**/android/gradlew.bat
frontend/**/android/local.properties
frontend/**/android/**/GeneratedPluginRegistrant.java

# iOS/XCode related
frontend/**/ios/**/*.mode1v3
frontend/**/ios/**/*.mode2v3
frontend/**/ios/**/*.moved-aside
frontend/**/ios/**/*.pbxuser
frontend/**/ios/**/*.perspectivev3
frontend/**/ios/**/*sync/
frontend/**/ios/**/.sconsign.dblite
frontend/**/ios/**/.tags*
frontend/**/ios/**/.vagrant/
frontend/**/ios/**/DerivedData/
frontend/**/ios/**/Icon?
frontend/**/ios/**/Pods/
frontend/**/ios/**/.symlinks/
frontend/**/ios/**/profile
frontend/**/ios/**/xcuserdata
frontend/**/ios/.generated/
frontend/**/ios/Flutter/App.framework
frontend/**/ios/Flutter/Flutter.framework
frontend/**/ios/Flutter/Flutter.podspec
frontend/**/ios/Flutter/Generated.xcconfig
frontend/**/ios/Flutter/ephemeral
frontend/**/ios/Flutter/app.flx
frontend/**/ios/Flutter/app.zip
frontend/**/ios/Flutter/flutter_assets/
frontend/**/ios/Flutter/flutter_export_environment.sh
frontend/**/ios/ServiceDefinitions.json
frontend/**/ios/Runner/GeneratedPluginRegistrant.*

# Coverage
frontend/coverage/

# Exceptions to above rules
!frontend/**/ios/**/default.mode1v3
!frontend/**/ios/**/default.mode2v3
!frontend/**/ios/**/default.pbxuser
!frontend/**/ios/**/default.perspectivev3

